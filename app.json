{
  "expo": {
    "name": "Expo Checkin",
    "slug": "expo-checkin",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#faae5e"
    },
    "assetBundlePatterns": ["**/*"],
    "scheme": "checkin",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.serendipitytech.checkin",
      "buildNumber": "1.0.0",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#faae5e"
      },
      "package": "com.serendipitytech.checkin",
      "versionCode": 1,
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "checkin",
              "host": "auth"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": ["expo-router"],
    "extra": {
      "eas": {
        "projectId": "4ec28d73-3633-44be-82d4-fef18e87a82e"
      }
    },
    "owner": "serendipitytech"
  }
}
